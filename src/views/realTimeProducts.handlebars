<main>
  <h1>Productos actualizados</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>description</th>
        <th>Code</th>
        <th>Price</th>
        <th>Status</th>
        <th>Stock</th>
        <th>Category</th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
      <tr>
        <td>{{ this.id }} </td>
        <td>{{ this.title }} </td>
        <td>{{ this.description }} </td>
        <td>{{ this.code }} </td>
        <td>{{ this.price }} </td>
        <td>{{ this.status }} </td>
        <td>{{ this.stock }} </td>
        <td>{{ this.category }} </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  
</main>
<script src="/socket.io/socket.io.js"></script>
<script src="/static/assets/js/index.js"></script>