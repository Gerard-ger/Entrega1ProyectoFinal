<h1>Productos</h1>
<table>
    <thead>
        <tr>
            <th>Imagen de Producto</th>
            <th>ID</th>
            <th>Title</th>
            <th>description</th>
            <th>Code</th>
            <th>Price</th>
            <th>Status</th>
            <th>Stock</th>
            <th>Category</th>
        </tr>
    </thead>
    <tbody>
        {{#each products}}
        <tr>
            <td><img src={{ this.thumbnail }} class="img-pequena"></td>
            <td>{{ this.id }} </td>
            <td>{{ this.title }} </td>
            <td>{{ this.description }} </td>
            <td>{{ this.code }} </td>
            <td>{{ this.price }} </td>
            <td>{{ this.status }} </td>
            <td>{{ this.stock }} </td>
            <td>{{ this.category }} </td>
        </tr>
        {{/each}}
    </tbody>
</table>
